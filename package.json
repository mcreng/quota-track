{
  "name": "quota-track",
  "version": "1.0.0",
  "description": "Data visualization on enrollment details in HKUST",
  "scripts": {
    "client-start": "cd client && npm start",
    "build": "cd client && npm build",
    "server-start": "nodemon server.js -i data/",
    "dev": "concurrently --kill-others-on-fail \"npm run server-start \" \"npm run client-start\"",
    "start": "node server.js",
    "postinstall": "cd client && npm install"
  },
  "author": "mcreng",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcreng/quota-track.git"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonfile": "^5.0.0",
    "log4js": "^3.0.6",
    "moment": "^2.22.2",
    "node-schedule": "^1.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/express": "^4.16.0",
    "@types/jsonfile": "^4.0.1",
    "@types/node-schedule": "^1.2.2",
    "@types/request-promise": "^4.1.42",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.5"
  }
}
